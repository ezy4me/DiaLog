{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "types": ["@types/react-native-dotenv"],
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@env": ["./.env"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
